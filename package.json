{
  "name": "@integral/contracts",
  "description": "TWAP smart contracts.",
  "license": "GPL-3.0-or-later",
  "version": "0.1.0",
  "engines": {
    "node": ">=14",
    "yarn": "^1.22.17"
  },
  "scripts": {
    "lint": "yarn lint:prettier --check && yarn lint:eslint",
    "lint:fix": "yarn lint:prettier --write && yarn lint:eslint --fix",
    "lint:eslint": "eslint './{test,playground,deploy,scripts}/**/*.ts'",
    "lint:prettier": "yarn prettier './{test,contracts,deploy,playground,scripts}/**/*.{ts,sol}'",
    "clean": "rimraf ./build && hardhat clean",
    "clean:deep": "rimraf ./build && hardhat clean --global",
    "prebuild": "yarn clean",
    "build": "hardhat compile",
    "test": "mocha",
    "deploy:amm:polygon": "DEFAULT_NETWORK='polygon' ts-node deploy/amm/index.ts",
    "deploy:amm:polygon-mumbai": "DEFAULT_NETWORK='polygon-mumbai' ts-node deploy/amm/index.ts",
    "deploy:amm:arbitrum": "DEFAULT_NETWORK='arbitrum' ts-node deploy/amm/index.ts",
    "deploy:amm:arbitrum-goerli": "DEFAULT_NETWORK='arbitrum-goerli' ts-node deploy/amm/index.ts",
    "deploy:amm:optimism": "DEFAULT_NETWORK='optimism' ts-node deploy/amm/index.ts",
    "deploy:amm:optimism-goerli": "DEFAULT_NETWORK='optimism-goerli' ts-node deploy/amm/index.ts",
    "deploy:amm:goerli": "DEFAULT_NETWORK='goerli' ts-node deploy/amm/index.ts",
    "deploy:amm:sepolia": "DEFAULT_NETWORK='sepolia' ts-node deploy/amm/index.ts",
    "deploy:amm:ropsten": "DEFAULT_NETWORK='ropsten' ts-node deploy/amm/index.ts",
    "deploy:amm:kovan": "DEFAULT_NETWORK='kovan' ts-node deploy/amm/index.ts",
    "deploy:amm:rinkeby": "DEFAULT_NETWORK='rinkeby' ts-node deploy/amm/index.ts",
    "deploy:amm:ganache": "DEFAULT_NETWORK='ganache' ts-node deploy/amm/index.ts",
    "deploy:amm:mainnet": "DEFAULT_NETWORK='mainnet' ts-node deploy/amm/index.ts",
    "restore-files": "ts-node deploy/amm/restoreSolFiles.ts",
    "deploy:token": "ts-node deploy/token/index.ts",
    "deploy:relayer:mainnet": "DEFAULT_NETWORK='mainnet' ts-node deploy/relayer/index.ts",
    "deploy:relayer:goerli": "DEFAULT_NETWORK='goerli' ts-node deploy/relayer/index.ts",
    "deploy:relayer:sepolia": "DEFAULT_NETWORK='sepolia' ts-node deploy/relayer/index.ts",
    "deploy:relayer:ganache": "DEFAULT_NETWORK='ganache' ts-node deploy/relayer/index.ts",
    "deploy:limitOrder:goerli": "DEFAULT_NETWORK='goerli' ts-node deploy/limitOrder/index.ts",
    "deploy:limitOrder:sepolia": "DEFAULT_NETWORK='sepolia' ts-node deploy/limitOrder/index.ts",
    "deploy:tenderly:amm:mainnet": "DEFAULT_NETWORK='mainnet' ts-node deploy/tenderly/amm.ts",
    "deploy:tenderly:relayer:mainnet": "DEFAULT_NETWORK='mainnet' ts-node deploy/tenderly/relayer.ts",
    "prepublishOnly": "yarn build && yarn test",
    "start": "ts-node playground/deploy.ts",
    "execute": "ts-node scripts/bot/executeSingle.ts",
    "monitor": "ts-node scripts/bot/monitor.ts",
    "bot": "ts-node scripts/bot/run.ts",
    "limitOrderBot": "ts-node scripts/bot/limitOrderRun.ts",
    "rebalancer": "ts-node scripts/rebalancer/run.ts",
    "ganache:start": "scripts/ganache/start.sh",
    "ganache:stop": "scripts/ganache/stop.sh",
    "ganache:list": "scripts/ganache/list.sh",
    "ganache:setupRebalancerTest": "ts-node scripts/ganache/setupRebalancerTest.ts",
    "enable": "ts-node scripts/disables/enable.ts",
    "disable": "ts-node scripts/disables/disable.ts",
    "set-owner": "ts-node scripts/owner/index.ts",
    "set-minter": "ts-node scripts/minter/index.ts",
    "set-trader": "ts-node scripts/trader/index.ts",
    "read-delay": "ts-node scripts/readDelay/index.ts"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@resolver-engine/imports-fs": "^0.3.3",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.3",
    "@types/argparse": "^2.0.2",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "argparse": "^2.0.1",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "chalk": "^4.1.0",
    "csv-parser": "^3.0.0",
    "eslint": "^7.15.0",
    "ethereum-waffle": "^4.0.10",
    "ethereumjs-util": "^7.0.7",
    "ethers": "^5.7.1",
    "ganache": "^7.7.6",
    "hardhat": "^2.11.2",
    "hardhat-abi-exporter": "^2.3.0",
    "hardhat-gas-reporter": "^1.0.4",
    "mocha": "^8.2.1",
    "prettier": "^2.1.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.59",
    "querystring": "^0.2.0",
    "rimraf": "^3.0.2",
    "solc": "^0.7.4",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@slack/bolt": "^3.12.2",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.1",
    "dotenv": "^8.2.0"
  }
}
