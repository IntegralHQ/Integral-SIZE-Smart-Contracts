{
  "name": "@integral/contracts",
  "description": "TWAP smart contracts.",
  "license": "GPL-3.0-or-later",
  "version": "0.1.0",
  "engines": {
    "node": ">=10",
    "yarn": "^1.17.3"
  },
  "scripts": {
    "lint": "yarn lint:prettier --check && yarn lint:eslint",
    "lint:fix": "yarn lint:prettier --write && yarn lint:eslint --fix",
    "lint:eslint": "eslint './{test,playground,deploy,scripts}/**/*.ts'",
    "lint:prettier": "yarn prettier './{test,contracts,deploy,playground,scripts}/**/*.{ts,sol}'",
    "clean": "rimraf ./build",
    "prebuild": "yarn clean",
    "build": "hardhat compile",
    "test": "mocha",
    "deploy:amm:ropsten": "DEFAULT_NETWORK='ropsten' ts-node deploy/amm/index.ts",
    "deploy:amm:kovan": "DEFAULT_NETWORK='kovan' ts-node deploy/amm/index.ts",
    "deploy:amm:rinkeby": "DEFAULT_NETWORK='rinkeby' ts-node deploy/amm/index.ts",
    "deploy:amm:mainnet": "DEFAULT_NETWORK='mainnet' ts-node deploy/amm/index.ts",
    "restore-files": "ts-node deploy/amm/restoreSolFiles.ts",
    "deploy:token": "ts-node deploy/token/index.ts",
    "prepublishOnly": "yarn build && yarn test",
    "start": "ts-node playground/deploy.ts",
    "execute": "ts-node scripts/bot/executeSingle.ts",
    "monitor": "ts-node scripts/bot/monitor.ts",
    "bot": "ts-node scripts/bot/run.ts",
    "enable": "ts-node scripts/disables/enable.ts",
    "disable": "ts-node scripts/disables/disable.ts",
    "set-owner": "ts-node scripts/owner/index.ts",
    "set-minter": "ts-node scripts/minter/index.ts",
    "set-trader": "ts-node scripts/trader/index.ts",
    "read-delay": "ts-node scripts/readDelay/index.ts"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@resolver-engine/imports-fs": "^0.3.3",
    "@typechain/ethers-v5": "^8.0.2",
    "@typechain/hardhat": "^3.0.0",
    "@types/argparse": "^2.0.2",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "argparse": "^2.0.1",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "chalk": "^4.1.0",
    "csv-parser": "^3.0.0",
    "eslint": "^7.15.0",
    "ethereum-waffle": "^3.4.0",
    "ethereumjs-util": "^7.0.7",
    "ethers": "^5.4.0",
    "hardhat": "^2.6.4",
    "hardhat-abi-exporter": "^2.3.0",
    "hardhat-gas-reporter": "^1.0.4",
    "mocha": "^8.2.1",
    "prettier": "^2.1.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.59",
    "querystring": "^0.2.0",
    "rimraf": "^3.0.2",
    "solc": "^0.7.4",
    "ts-node": "^9.0.0",
    "typechain": "^6.0.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "dotenv": "^8.2.0"
  }
}
